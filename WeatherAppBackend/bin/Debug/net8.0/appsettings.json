{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },

    "AllowedHosts": "*",
    "MongoDB": {
        "ConnectionString": "your-connection-string",
        "DatabaseName": "WeatherDataNew",
        "FavoritesCollection": "FavouriteCitiesNew"
    },

    "Jwt": {
        "Key": "your-jwt-key",
        "Issuer": "WeatherApp",
        "Audience": "WeatherAppUsers"
    },

    "OpenWeather": {
        "ApiKey": "api-key",
        "BaseUrl": "https://api.openweathermap.org/data/2.5/"
    },

    
    "Unsplash": {
        "AccessKey": "api-key",
        "BaseUrl": "https://api.unsplash.com/"
    }

}
